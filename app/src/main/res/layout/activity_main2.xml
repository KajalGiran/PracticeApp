<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main2Activity">

    <com.example.practiceapp.CircleSeekBar
        android:id="@+id/inner_circle"
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:wheel_can_touch="false"
        app:wheel_cur_process="50"
        app:wheel_max_process="100"
        app:wheel_reached_width="7dp"
        app:wheel_unreached_width="7dp" />


    <com.example.practiceapp.CircleSeekBar
        android:id="@+id/outer_circle"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:wheel_can_touch="false"
        app:wheel_cur_process="50"
        app:wheel_max_process="100"
        app:wheel_reached_width="7dp"
        app:wheel_unreached_width="7dp" />
</androidx.constraintlayout.widget.ConstraintLayout>